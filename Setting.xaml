        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="设置" Height="300" Width="400" Icon="/Gait;component/Images/Setting.png" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
