        <Label Content="播放速率：" Height="28" HorizontalAlignment="Left" Margin="53,40,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="129,42,0,0" Name="textBox1" VerticalAlignment="Top" Width="97" Text="10" />
        <Label Content="帧/秒(fps)" Height="28" HorizontalAlignment="Left" Margin="237,40,0,0" Name="label2" VerticalAlignment="Top" />
        <Button Content="确定" Height="23" HorizontalAlignment="Left" Margin="72,213,0,0" Name="button1" VerticalAlignment="Top" Width="75" IsDefault="True" />
        <Button Content="取消" Height="23" HorizontalAlignment="Left" Margin="203,213,0,0" Name="button2" VerticalAlignment="Top" Width="75" IsCancel="True" />
    </Grid>
</Window>
